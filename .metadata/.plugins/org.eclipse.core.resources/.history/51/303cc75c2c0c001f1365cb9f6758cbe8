import jakarta.servlet.http.HttpServletRequest;

@Controller
@RequestMapping("param")
public class ParamController {
	@Autowired
	HttpServletRequest req;
	
	@RequestMapping("form")
	public String form() {
		return "form";
	}
	
	@RequestMapping("save/{x}")
	public String save(/*?1*/String x,/*?2*/String y){
		return "form";
	}
	
}
